""" ;
    dc:identifier "341" ;
    uni:subjectOf "MANA" ;
    foaf:name "Organization Theory and Design" .

unidata:MANA343 a uni:Course ;
    dc:description """Prerequisite: COMM 222 previously or concurrently; or MANA 201 and
